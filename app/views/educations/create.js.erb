<% if @education.valid? %>
  $('#education-errors').html("");
  $('#education-display').html("<%= j render :partial => './users/education_display' %>");
  $('#education-display').children().last().hide();
  $('#education-display').children().last().fadeIn('fast');
<% else %>
  $('#education-errors').html("<%= j(render :partial => 'shared/error_messages', locals: {obj: @education}) %>");
<% end %>
